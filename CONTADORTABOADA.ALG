algoritmo "contadorTaboada"
var

n , cont , s : inteiro




inicio
      cont <- 0

      escreva("digite um numero  para ver sua taboada: ")
      leia(n)
      
      repita

            s <- n * cont
            escreval(n , " X " , cont , " = " , s )
            cont <- cont + 1

      ate (cont >= 11)
      








fimalgoritmo